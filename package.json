{
	"name": "@crevendo/admin-kit",
	"version": "0.0.7",
	"publishConfig": {
		"@crevendo:registry": "https://npm.pkg.github.com"
	},
	"repository": "https://github.com/crevendo/admin-kit",
	"scripts": {
		"dev": "vite dev",
		"build": "npx tailwindcss -o ./src/lib/build.css --minify && svelte-kit sync && svelte-package && cp .npmrc dist/.npmrc",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@sveltejs/package": "^2.0.2",
		"autoprefixer": "^10.4.13",
		"postcss": "^8.4.21",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tailwindcss": "^3.2.6",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module",
	"exports": {
		"./package.json": "./package.json",
		"./build.css": "./package/build.css",
		"./components/Button.svelte": {
			"types": "./dist/components/Button.svelte.d.ts",
			"svelte": "./dist/components/Button.svelte",
			"default": "./dist/components/Button.svelte"
		},
		"./components/Coloumn.svelte": {
			"types": "./dist/components/Coloumn.svelte.d.ts",
			"svelte": "./dist/components/Coloumn.svelte",
			"default": "./dist/components/Coloumn.svelte"
		},
		"./components/Input.svelte": {
			"types": "./dist/components/Input.svelte.d.ts",
			"svelte": "./dist/components/Input.svelte",
			"default": "./dist/components/Input.svelte"
		},
		"./components/List.svelte": {
			"types": "./dist/components/List.svelte.d.ts",
			"svelte": "./dist/components/List.svelte",
			"default": "./dist/components/List.svelte"
		},
		"./components/Row.svelte": {
			"types": "./dist/components/Row.svelte.d.ts",
			"svelte": "./dist/components/Row.svelte",
			"default": "./dist/components/Row.svelte"
		},
		"./components/Section.svelte": {
			"types": "./dist/components/Section.svelte.d.ts",
			"svelte": "./dist/components/Section.svelte",
			"default": "./dist/components/Section.svelte"
		},
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"svelte": "./dist/index.js"
}
